@model IEnumerable<HotelFlightMVC.Models.HotelRoom>
@{
    ViewData["Title"] = "Available Hotel Rooms";
}

<header>
    <nav id="navbar">
        <div class="container">
            <h1 class="logo"><a href="@Url.Action("Index", "Home")">HotelFlightBooking</a></h1>
            <ul>
                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                <li><a href="@Url.Action("About", "Home")">About</a></li>
                <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
                <li><a href="@Url.Action("Privacy", "Home")">Privacy</a></li>
            </ul>
        </div>
    </nav>
</header>

<div class="container my-5">
    <h1 class="text-center">Available Hotel Rooms</h1>
    <div class="row">
        @if (Model != null && Model.Any())
        {
            foreach (var room in Model)
            {
                <div class="col-md-4 mb-4">
                    <div class="card shadow-sm">
                        <img src="data:image/jpeg;base64," class="card-img-top" alt="Room Image">
                        <div class="card-body">
                            <h5 class="card-title">@room.Name</h5>
                            <p class="card-text">Room Type: @room.RoomType</p>
                            <p class="card-text">Capacity: @room.Capacity</p>
                            <p class="card-text">Price: $@room.PricePerNight per night</p>
                            <a href="@Url.Action("Buy", new { id = room.Id })" class="btn btn-primary btn-block">Buy Hotel Room</a>
                           @*  <form asp-action="Delete" asp-route-id="@room.Id" method="post">
                                <button type="submit" class="btn btn-danger">Delete</button>
                            </form> *@

                        </div>
                    </div>
                </div>
            }
        }
        else
        {
            <p class="text-center">No hotel rooms available at the moment.</p>
        }
    </div>
</div>
